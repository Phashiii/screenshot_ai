
{
    "manifest_version":2,
    "name":"ScreenAI",
    "version": "1.0",
    "permissions": ["activeTab", "<all_urls>", "storage"],
    "background": {
        "scripts":["background.js"],
        "persistent": false
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["content.js"]
        }
    ],
    "browser_action":{
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "web_accessible_resources":[
        "overlay.js",
        "styles/overlay.css"
    ]
}
